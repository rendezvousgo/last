      "slowPeriod": 26,
      "signalPeriod": 9
    },
    "MACD_HISTOGRAM": {
      "description": "MACD - Signal Line"
    },
    "EMA20": {
      "description": "20-period Exponential Moving Average",
      "period": 20
    },
    "EMA50": {
      "description": "50-period Exponential Moving Average",
      "period": 50
    },
    "BB_POSITION": {
      "description": "Price position within Bollinger Bands (0-100)",
      "period": 20,
      "stdDev": 2
    },
    "BB_BANDWIDTH": {
      "description": "Bollinger Band width (%)"
    },
    "FEAR_GREED": {
      "description": "Fear & Greed Index (0-100)",
      "source": "alternative.me"
    },
    "VOLUME_SURGE": {
      "description": "Volume surge detection",
      "threshold": "150% of average"
    }
  },
  "operators": {
    ">": "greater than",
    "<": "less than",
    ">=": "greater than or equal",
    "<=": "less than or equal",
